{
    "name": "huttopia/doctrine",
    "license": "MIT",
    "description": "Add some features and fix to doctrine, without forking it",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Huttopia\\Doctrine\\": "/"
        }
    },
    "require": {
        "php": ">=7.1.3",

        "doctrine/orm": "2.5.15",
        "doctrine/doctrine-bundle": "3.0",

        "steevanb/doctrine-read-only-hydrator": "^2.2",
        "steevanb/doctrine-entity-merger": "^1.0",
        "steevanb/doctrine-events": "^1.2",

        "steevanb/composer-overload-class": "^1.3"
    },
    "require-dev": {
        "steevanb/doctrine-stats": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Huttopia/DoctrineBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Huttopia/orm.git"
        }
    ]
}
